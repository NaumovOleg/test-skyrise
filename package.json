{
  "name": "skyrise",
  "version": "0.1.0",
  "bin": {
    "skyrise": "bin/skyrise.js"
  },
  "scripts": {
    "build": "tsc",
    "postinstall": "node-config-ts",
    "start": "nodemon",
    "lint": "eslint  \"*/**/*.{js,ts}\"",
    "lint:fix": "eslint  --fix \"*/**/*.{js,ts}\"",
    "prepare": "husky install",
    "cdk": "NODE_ENV=develop DEPLOYMENT=develop cdk",
    "emit-for-sam": "cdk synth --no-staging  > template.yaml --app \"ts-node --prefer-ts-exts ./bin/local.ts\""
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/aws-lambda": "^8.10.114",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "aws-cdk": "2.72.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.37.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "nodemon": "^2.0.22",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.73.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.73.0-alpha.0",
    "aws-cdk-lib": "2.72.1",
    "aws-lambda": "^1.0.7",
    "constructs": "^10.1.301",
    "moment": "^2.29.4",
    "node-config-ts": "^3.3.1",
    "source-map-support": "^0.5.21"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
